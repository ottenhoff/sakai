    <name>Sakai E2E Tests</name>
    <groupId>org.sakaiproject</groupId>
    <artifactId>sakai-e2e-tests</artifactId>
    <packaging>jar</packaging>

    <!-- No Java dependencies needed - using Node.js Playwright directly -->

    <profiles>
        <profile>
            <id>e2e-tests</id>
            <properties>
                <sakai.baseUrl>http://localhost:9080</sakai.baseUrl>
                <sakai.testUser>admin</sakai.testUser>
                <sakai.testPassword>admin</sakai.testPassword>
            </properties>
        </profile>
    </profiles>
</project>
